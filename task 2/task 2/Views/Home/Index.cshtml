@{
    Layout = "_Layout";
}

 <h1>Extraordinary rooms with elegant, yet a rustic atmoshere</h1>


@{
    string[] roomName = { "Apartment with amazing Park View", "Double Deluxe room-Double Bed", "Luxury Apartment - Ground Floor" };
    int[] roomPrice = { 45, 35, 40 };
    string[] roomImage = { "data:image/jpeg;base64,",
    "data:image/jpeg;base64,",
    "data:image/jpeg;base64," };
}

<div class="d-flex flex-wrap gap-3 justify-content-center">

@for (int i = 0; i < roomName.Length; i++)
{



    <div class="card" style="width: 18rem;">
        <img class="card-img-top" src="@roomImage[i]" alt="Card image cap">
        <div class="card-body">
            <h5 class="card-title">@roomName[i]</h5>
            <p class="card-text">$@roomPrice[i]</p>
            <a href="#" >VIEW</a>
        </div>
    </div>

}
</div>
