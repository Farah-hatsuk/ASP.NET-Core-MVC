
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<h1>User List</h1>


@{
    string[] username = { "farah", "hani", "ammar", "toqa" };
    string[] email = { "farah@gmail.com", "hani@gmail.com", "ammar@gmail.com", "toqa@gmail.com" };
    int[] age = { 20, 21, 22, 23 };
}
<table class="table">
    <thead>
        <tr>
            <th scope="col">username</th>
            <th scope="col">email</th>
            <th scope="col">age</th>
        </tr>
    </thead>
    <tbody>



        @for (int i = 0; i < username.Length; i++)
        {

            <tr>

                <td>@username[i]</td>
                <td>@email[i]</td>
                <td>@age[i]</td>

            </tr>



        }


    </tbody>
</table>


