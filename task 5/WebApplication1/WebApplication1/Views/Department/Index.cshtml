

@model IEnumerable<WebApplication1.Models.Department>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table table-bordered ">

    <thead>
        <tr>
            @* <th>@Html.DisplayNameFor(model => model.First().Id)</th> *@
            <th>@Html.DisplayNameFor(model => model.First().Name)</th>
            <th>@Html.DisplayNameFor(model => model.First().Location)</th>
            <th>@Html.DisplayNameFor(model => model.First().Manager)</th>
            <th>@Html.DisplayNameFor(model => model.First().EmployeesCount)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
               @*  <td>@item.Id</td> *@
                <td>@item.Name</td>
                <td>@item.Location</td>
                <td>@item.Manager</td>
                <td>@item.EmployeesCount</td>

            </tr>
        }
    </tbody>
</table>


<a asp-controller="Department" asp-action="Create">Add New Department</a>

